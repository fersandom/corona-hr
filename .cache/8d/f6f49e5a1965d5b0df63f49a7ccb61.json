erChart();\n      renderData();\n    });\n}\n\n//Renders the data from the table\nfunction renderData() {\n  for (let i = 0; i < 30; i++) {\n    let output = document.createElement(\"tr\");\n    output.innerHTML = `\n        <td class=\"table-data\">${dates[i]}</td>\n        <td class=\"table-data\">${dayZero[i].Cases}</td>\n        <td class=\"table-data\">${diffArray[i]}</td>\n    `;\n\n    table.appendChild(output);\n  }\n}\n\nfunction renderChart() {\n  let ctx = document.getElementById(\"confirmedCases\").getContext(\"2d\");\n  let chart = new Chart(ctx, {\n    // The type of chart we want to create\n    type: \"line\",\n\n    // The data for our dataset\n    data: {\n      labels: dates,\n      datasets: [\n        {\n          label: \"Confirmed cases\",\n          borderColor: \"rgb(125, 70, 0)\",\n          data: totalCases,\n        },\n      ],\n    },\n\n    // Configuration options go here\n    options: {},\n  });\n\n   ctx = document.getElementById(\"dailyIncrease\").getContext(\"2d\");\n   chart = new Chart(ctx, {\n    // The type of chart we want to create\n    type: \"bar\",\n\n    // The data for our dataset\n    data: {\n      labels: dates,\n      datasets: [\n        {\n          label: \"daily cases\",\n          backgroundColor: \"rgb(125, 70, 0)\",\n          data: diffArrayChart,\n        },\n      ],\n    },\n\n    // Configuration options go here\n    options: {},\n  });\n}\n\n//Calls the first function\nfetchData();\n"},"lineCount":null}},"error":null,"hash":"2403bca293cbd5c71ec43f766f22a15b","cacheData":{"env":{}}}